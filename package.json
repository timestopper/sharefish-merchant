{
  "name": "sf",
  "version": "0.0.1",
  "repository": "https://github.com/VladimirPal/sf.git",
  "config": {
    "buildDir": "./build",
    "devHost": "localhost",
    "devPort": 3000
  },
  "scripts": {
    "production": "NODE_ENV=production npm run webpack",
    "staging": "NODE_ENV=staging npm run webpack",
    "clean": "rimraf $npm_package_config_buildDir && mkdir $npm_package_config_buildDir",
    "env": "env",
    "lint": "eslint --ext .js ./ && echo No linting errors.",
    "prebuild": "npm run clean",
    "prestart": "npm install",
    "start": "NODE_ENV=development node dev-server ./webpack/config",
    "webpack": "webpack --colors --progress --config ./webpack/config"
  },
  "dependencies": {
    "bootstrap": "^3.3.6",
    "classnames": "^2.1.3",
    "font-awesome": "^4.5.0",
    "history": "^2.0.0",
    "jquery": "^2.2.0",
    "ladda": "^0.9.8",
    "moment": "^2.11.2",
    "normalize.css": "^3.0.3",
    "react": "^0.14.7",
    "react-bootstrap": "^0.28.3",
    "react-dom": "^0.14.7",
    "react-ladda": "^4.0.0",
    "react-maskedinput": "^3.1.0",
    "react-redux": "^4.0.5",
    "react-router": "^2.0.0-rc5",
    "redux": "^3.2.1",
    "redux-thunk": "^1.0.2"
  },
  "devDependencies": {
    "autoprefixer-core": "^6.0.1",
    "babel-core": "^6.4.5",
    "babel-eslint": "^4.1.8",
    "babel-loader": "^6.2.2",
    "babel-plugin-rewire": "^0.1.8",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-runtime": "^6.3.19",
    "chai": "^3.0.0",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.16.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.4",
    "glob": "^6.0.4",
    "html-loader": "^0.4.0",
    "json-loader": "^0.5.2",
    "less": "^2.6.0",
    "less-loader": "^2.2.2",
    "less-plugin-clean-css": "^1.5.1",
    "lodash": "^4.2.1",
    "mocha": "^2.2.5",
    "mocha-loader": "^0.7.1",
    "node-libs-browser": "^1.0.0",
    "opn": "^4.0.0",
    "postcss-loader": "^0.8.0",
    "react-hot-loader": "^1.2.8",
    "redux-devtools": "^3.0.1",
    "redux-devtools-dock-monitor": "^1.1.0",
    "redux-devtools-log-monitor": "^1.0.1",
    "rimraf": "^2.4.1",
    "sinon": "^1.15.4",
    "source-map-support": "^0.4.0",
    "style-loader": "^0.13.0",
    "template-html-loader": "0.0.3",
    "webpack": "^1.12.3",
    "webpack-dev-server": "^1.10.1"
  },
  "engines": {
    "node": ">=0.12.0"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "ecmaFeatures": {
      "modules": true,
      "jsx": true
    },
    "globals": {
      "describe": true,
      "it": true,
      "sinon": true
    },
    "parser": "babel-eslint",
    "plugins": [
      "react"
    ],
    "rules": {
      "strict": [
        2,
        "global"
      ],
      "indent": [
        2,
        2
      ],
      "quotes": [
        2,
        "single"
      ],
      "no-alert": 0,
      "no-underscore-dangle": 0,
      "react/display-name": 0,
      "react/jsx-quotes": 1,
      "react/jsx-no-undef": 1,
      "react/jsx-sort-props": 1,
      "react/jsx-uses-react": 1,
      "react/jsx-uses-vars": 1,
      "react/no-did-mount-set-state": 1,
      "react/no-did-update-set-state": 1,
      "react/no-multi-comp": 0,
      "react/no-unknown-property": 1,
      "react/prop-types": 0,
      "react/react-in-jsx-scope": 1,
      "react/self-closing-comp": 1,
      "react/wrap-multilines": 1
    }
  }
}
